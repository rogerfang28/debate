{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake Configure (Debug)",
      "type": "shell",
      "command": "cmake",
      "args": [
        "-S", "${workspaceFolder}/cppserver",
        "-B", "${workspaceFolder}/cppserver/build",
        "-DCMAKE_BUILD_TYPE=Debug"
      ],
      "options": {
        "cwd": "${workspaceFolder}/cppserver"
      },
      "problemMatcher": []
    },
    {
      "label": "CMake Build (Debug)",
      "type": "shell",
      "command": "cmake",
      "args": [
        "--build", "${workspaceFolder}/cppserver/build",
        "--config", "Debug",
        "-j"
      ],
      "options": {
        "cwd": "${workspaceFolder}/cppserver"
      },
      "dependsOn": "CMake Configure (Debug)",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
