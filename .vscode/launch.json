{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug C++ (MinGW gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,

      "MIMode": "gdb",
      "miDebuggerPath": "C:/mingw-w64/mingw64/bin/gdb.exe",

      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],

    //   "preLaunchTask": "Build active C++ file (dynamic)"
    }
  ]
}
